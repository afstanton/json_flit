{
  "type": "scaffold",
  "args": {
    "appBar": {
      "type": "app_bar",
      "args": {
        "title": {
          "type": "text",
          "args": {
            "text": "TweenAnimation"
          }
        }
      }
    },
    "body": {
      "type": "set_value",
      "args": {
        "customSize": 50.0
      },
      "child": {
        "type": "center",
        "child": {
          "type": "tween_animation",
          "args": {
            "builder": "${getCustomTweenBuilder()}",
            "duration": 1000,
            "tween": "${getCustomTween(customSize)}"
          },
          "child": {
            "type": "icon",
            "args": {
              "color": "#0000FF",
              "icon": {
                "codePoint": "0xe5db",
                "fontFamily": "MaterialIcons"
              }
            }
          }
        }
      }
    }
  }
}